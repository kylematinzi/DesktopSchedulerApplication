      <Label layoutX="65.0" layoutY="90.0" text="Name:" />
      <Label layoutX="66.0" layoutY="156.0" text="Username: " />
      <Label layoutX="68.0" layoutY="223.0" text="Password: " />
      <TextField fx:id="nameTextBox" layoutX="67.0" layoutY="107.0" promptText="Your Name" />
      <TextField fx:id="usernameTextBox" layoutX="67.0" layoutY="173.0" promptText="New Username" />
      <TextField fx:id="passwordTextBox" layoutX="67.0" layoutY="240.0" promptText="Password" />
   </children>
</AnchorPane>
